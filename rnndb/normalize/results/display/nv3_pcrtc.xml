<?xml version="1.0" encoding="UTF-8"?>
<database xmlns:n1="http://www.w3.org/2001/XMLSchema-instance" n1:schemaLocation="http://nouveau.freedesktop.org/ rules-ng.xsd">
    <import file="copyright.xml"/>
    <import file="nvchipsets.xml"/>
    <import file="display/nv_vga.xml"/>
    <group name="nv_pcrtc">
        <reg32 offset="0x100" name="INTR">
            <bitfield name="VBLANK" pos="0"/>
        </reg32>
        <reg32 offset="0x140" name="INTR_EN">
            <bitfield name="VBLANK" pos="0"/>
        </reg32>
    </group>
    <domain name="NV_MMIO" bare="yes" prefix="chipset">
        <array name="PCRTC" offset="0x600000" stride="0x1000" length="1" variants="NV4:G80">
            <use-group name="nv_pcrtc"/>
        </array>
        <array name="PRMCIO" offset="0x601000" stride="0x1000" length="1" variants="NV3:G80">
            <use-group name="nv_vga_cio"/>
        </array>
        <array name="PCRTC2" offset="0x602000" stride="0x1000" length="1" variants="NV11:G80">
            <use-group name="nv_pcrtc"/>
        </array>
        <array name="PRMCIO2" offset="0x603000" stride="0x1000" length="1" variants="NV11:G80">
            <use-group name="nv_vga_cio"/>
        </array>
    </domain>
</database>
