<?xml version="1.0" encoding="UTF-8"?>
<database xmlns:n1="http://www.w3.org/2001/XMLSchema-instance" n1:schemaLocation="http://nouveau.freedesktop.org/ rules-ng.xsd">
    <import file="copyright.xml"/>
    <import file="nvchipsets.xml"/>
    <import file="falcon.xml"/>
    <import file="falcon_crypt.xml"/>
    <domain name="NV_MMIO" bare="yes" prefix="chipset">
        <bitset name="GF119_PUNK1C3_INTR" varset="chipset">
            <bitfield low="0" high="7" name="FALCON" type="falcon_intr"/>
            <bitfield pos="9" name="MEMIF_BREAK"> XXX </bitfield>
        </bitset>
        <bitset name="GF119_PUNK1C3_STATUS" varset="chipset">
            <bitfield pos="0" name="FALCON"/>
            <bitfield pos="1" name="MEMIF"/>
        </bitset>
        <array name="PUNK1C3" offset="0x1c2000" stride="0x1000" length="1" variants="GF119-">
            <use-group name="falcon_base"/>
            <use-group name="falcon_memif"/>
            <use-group name="falcon_crypt"/>
            <reg32 offset="0xa00" name="UNKA00" length="0x80"/>
        </array>
    </domain>
</database>
