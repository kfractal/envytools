    <enum name="G80_COMPRESSION">
        <value value="0" name="NONE"/>
        <value value="1" name="SINGLE"/>
        <value value="2" name="DOUBLE"> valid only in DMAobj </value>
        <value value="3" name="USEVM"/>
    </enum>
    <domain name="G80_DMAOBJ" size="0x10" varset="chipset">
        <reg32 offset="0x0" name="FLAGS">
            <bitfield low="0" high="15" name="CLASS"> don't you love how every target field has sorta the same, but really different set of allowed values... </bitfield>
            <bitfield low="16" high="17" name="TARGET">
                <value value="0" name="VM"/>
                <value value="1" name="VRAM"/>
                <value value="2" name="SYSRAM"/>
                <value value="3" name="SYSRAM_NO_SNOOP"/>
            </bitfield>
            <bitfield low="18" high="19" name="ACCESSRW">
                <value value="0" name="USEVM"/>
                <value value="1" name="READ_ONLY"/>
                <value value="2" name="READ_WRITE"/>
            </bitfield>
            <bitfield low="20" high="21" name="ACCESSUS">
                <value value="0" name="USEVM"/>
                <value value="1" name="USER_SUPERVISOR"/>
                <value value="2" name="SUPERVISOR_ONLY"/>
            </bitfield>
            <bitfield low="22" high="28" name="STORAGE_TYPE" type="G80_STORAGE_TYPE"/>
            <bitfield low="29" high="30" name="COMPRESSION" type="G80_COMPRESSION"/>
            <bitfield pos="31" name="SUPERVISOR"/>
        </reg32>
        <reg32 offset="0x4" name="LIMIT"/>
        <reg32 offset="0x8" name="START"/>
        <reg32 offset="0xc" name="EXTRA">
            <bitfield low="0" high="7" name="START_HIGH"/>
            <bitfield low="24" high="31" name="LIMIT_HIGH"/>
        </reg32>
        <reg32 offset="0x10" name="COMP">
            <bitfield low="0" high="11" name="TAG_START"/>
            <bitfield low="16" high="27" name="TAG_LIMIT"/>
        </reg32>
        <reg32 offset="0x14" name="EXTRA2">
            <bitfield low="0" high="15" name="COMP_VRAM_START" shr="16"> XXX: figure out wtf this is about and why it only works on some cards </bitfield>
            <bitfield low="16" high="17" name="PART_CYCLE">
                <value value="0" name="USEVM"/>
                <value value="1" name="SHORT"/>
                <value value="2" name="LONG"/>
            </bitfield>
            <bitfield low="18" high="19" name="ENCRYPTED" variants="G84-">
                <value value="0" name="NO"/>
                <value value="1" name="YES"/>
                <value value="2" name="USEVM"/>
            </bitfield>
        </reg32>
    </domain>
    <bitset name="G80_PDE" varset="chipset">
        <bitfield low="0" high="1" name="PT_TYPE">
            <value value="0" name="NOT_PRESENT"/>
            <value value="1" name="64K"/>
            <value value="2" name="16K" variants="GT215-"/>
            <value value="3" name="4K"/>
        </bitfield>
        <bitfield low="2" high="3" name="TARGET" type="g80_mem_target"> 4? </bitfield>
        <bitfield low="5" high="6" name="PT_SIZE">
            <value value="0" name="FULL"/>
            <value value="1" name="128M"/>
            <value value="2" name="64M"/>
            <value value="3" name="32M"/>
        </bitfield>
        <bitfield low="12" high="39" name="ADDRESS" shr="12"/>
    </bitset>
    <bitset name="G80_PTE" varset="chipset">
        <bitfield pos="0" name="PRESENT"> 1?  2? </bitfield>
        <bitfield pos="3" name="READ_ONLY"/>
        <bitfield low="4" high="5" name="TARGET">
            <value value="0" name="VRAM"/>
            <value value="1" name="P2P"> address bits 38-39 select target port </value>
            <value value="2" name="SYSRAM"/>
            <value value="3" name="SYSRAM_NO_SNOOP"/>
        </bitfield>
        <bitfield pos="6" name="SUPERVISOR_ONLY"/>
        <bitfield low="7" high="9" name="CONTIG_BLOCK_LOG2"/>
        <bitfield low="12" high="39" name="ADDRESS" shr="12"/>
        <bitfield low="40" high="46" name="STORAGE_TYPE" type="G80_STORAGE_TYPE"/>
        <bitfield low="47" high="48" name="COMPRESSION" type="G80_COMPRESSION"/>
        <bitfield low="49" high="60" name="TAG"/>
        <bitfield pos="61" name="PART_CYCLE">
            <value value="0" name="SHORT"/>
            <value value="1" name="LONG"/>
        </bitfield>
        <bitfield pos="62" name="ENCRYPTED" variants="G84-"/>
    </bitset>
</database>
